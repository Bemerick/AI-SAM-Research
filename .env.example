# SAM.gov API
SAM_API_KEY=your_sam_api_key_here

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o

# Microsoft Teams
TEAMS_WEBHOOK_URL=your_teams_webhook_url_here

# Microsoft Graph / SharePoint
MS_TENANT_ID=your_tenant_id_here
MS_CLIENT_ID=your_client_id_here
MS_CLIENT_SECRET=your_client_secret_here
SHAREPOINT_SITE_URL=your_sharepoint_site_url_here
SHAREPOINT_LIST_NAME=your_list_name_here

# GovWin API
GOVWIN_USERNAME=your_govwin_username_here
GOVWIN_PASSWORD=your_govwin_password_here

# Microsoft Dynamics CRM (Optional - for CRM integration)
DYNAMICS_TENANT_ID=your_azure_tenant_id_here
DYNAMICS_CLIENT_ID=your_azure_app_client_id_here
DYNAMICS_CLIENT_SECRET=your_azure_app_client_secret_here
DYNAMICS_RESOURCE_URL=https://yourorg.crm.dynamics.com

# Database Configuration
# Local Development (with Docker)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/sam_govwin

# Azure Production Example (uncomment and update for production)
# DATABASE_URL=postgresql://username:password@your-server.postgres.database.azure.com:5432/sam_govwin?sslmode=require
